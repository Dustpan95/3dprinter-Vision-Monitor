# RTSP Stream Configuration
RTSP_STREAM_URL=rtsp://username:password@camera-ip:port/stream
RTSP_TIMEOUT=10

# MQTT Configuration
MQTT_BROKER_HOST=mqtt-broker-ip
MQTT_BROKER_PORT=1883
MQTT_USERNAME=your-mqtt-username
MQTT_PASSWORD=your-mqtt-password
MQTT_TOPIC_FAILURE=printer/failure
MQTT_TOPIC_HEARTBEAT=printer/heartbeat
MQTT_TOPIC_CONTROL=printer/control
MQTT_HEARTBEAT_INTERVAL=30
MQTT_CLIENT_ID=print-monitor

# Detection Settings
CHECK_INTERVAL_SECONDS=10
ML_API_URL=http://ml_api:3333
DETECTION_THRESHOLD=0.6
ML_API_TIMEOUT=15

# Standby Mode (VRAM Management)
STANDBY_MODE_ENABLED=true
STANDBY_AUTO_TIMEOUT=300
ML_API_CONTAINER_NAME=ml_api

# Web UI Configuration
WEB_PORT=8090

# Logging
LOG_LEVEL=INFO
